    "nx:serve:api": "npx nx run api:serve",
    "nx:serve:web": "npx nx run web:serve",
    "lint": "npx nx lint",
    "lint:tailwind": "npx nx lint --verbose --files=**/tailwind.config.js",
    "format-tailwind": "prettier --log-level warn --write \"**/tailwind.config.js\"",
    "json:server": "json-server --watch db/json/data.json --routes db/json/routes.json --no-cors=true",
    "json:proxy": "ng serve --proxy-config db/proxy.conf.json",
    "json:proxy:server": "concurrently --kill-others \"npm run json:server\" \"npm run json:proxy\""
  },
  "nx": {
    "includedScripts": [
      "nx:serve:api",
      "nx:serve:web",
      "lint:tailwind"
    ]
  },
